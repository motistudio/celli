{
  "name": "celli",
  "version": "1.2.5",
  "description": "A library for caching and memoization. This library provides advanced memoization utils, cache-creating and the ability to freely composing a custom cache.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "npx tsup src/index.ts --out-dir=../dist --tsconfig=tsconfig.lib.json --minify --dts --dts-resolve --external '' --format=\"esm,cjs\" --clean",
    "prepack": "npm run build",
    "typecheck": "npx tsc --noEmit --skipLibCheck --project ./tsconfig.lib.json",
    "typecheck:test": "npx tsc --noEmit --skipLibCheck --project ./tsconfig.spec.json",
    "test": "npx vitest run"
  },
  "keywords": [
    "cache",
    "save",
    "keep",
    "lru",
    "effects",
    "backup",
    "remote",
    "source",
    "cleanup",
    "lifecycle",
    "item",
    "custom",
    "compose",
    "composable",
    "memo",
    "memoize",
    "memoization",
    "context",
    "runtime",
    "server",
    "clean",
    "clear",
    "cacheManager",
    "dispose",
    "once",
    "decorator"
  ],
  "homepage": "https://motistudio.github.io/celli",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motistudio/celli.git"
  },
  "author": "Moti Shriki",
  "license": "ISC",
  "sideEffects": false
}
