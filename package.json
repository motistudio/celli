{
  "name": "celli",
  "version": "1.2.3",
  "description": "A library for caching and memoization. This library provides advanced memoization utils, cache-creating and the ability to freely composing a custom cache.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "workspaces": [
    "website"
  ],
  "scripts": {
    "build": "npx tsup src/index.ts --out-dir=dist --tsconfig=tsconfig.json --minify --dts --external '' --format=\"esm,cjs\" --clean",
    "typecheck": "npx tsc --noEmit --project tsconfig.lib.json",
    "typecheck:test": "npx tsc --noEmit --project tsconfig.test.json",
    "prepack": "npm run build",
    "lint": "npx eslint .",
    "test": "npx vitest run",
    "docs:start": "cd website && npm start",
    "docs:build": "cd website && npm run build",
    "docs:serve": "cd website && npm run serve",
    "docs:deploy": "cd website && npm run deploy"
  },
  "keywords": [
    "cache",
    "save",
    "keep",
    "lru",
    "effects",
    "backup",
    "remote",
    "source",
    "cleanup",
    "lifecycle",
    "item",
    "custom",
    "compose",
    "composable",
    "memo",
    "memoize",
    "memoization",
    "context",
    "runtime",
    "server",
    "clean",
    "clear",
    "cacheManager",
    "dispose",
    "once",
    "decorator"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motistudio/celli.git"
  },
  "author": "Moti Shriki",
  "license": "ISC",
  "devDependencies": {
    "@docusaurus/core": "3.9.2",
    "@docusaurus/module-type-aliases": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@docusaurus/tsconfig": "3.9.2",
    "@docusaurus/types": "3.9.2",
    "@mdx-js/react": "^3.0.0",
    "@swc/core": "^1.8.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitest/coverage-v8": "^3.1.1",
    "clsx": "^2.0.0",
    "eslint": "8.48.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.3.5",
    "type-fest": "^5.1.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  }
}
